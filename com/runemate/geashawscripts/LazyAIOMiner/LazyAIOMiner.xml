<manifest>
    <main-class>com.runemate.geashawscripts.LazyAIOMiner.LazyAIOMiner</main-class>
    <name>Lazy AIO Miner</name>
    <description>Mines ores in various locations. Dropping or banking them.</description>
    <version>0.65</version>
    <open-source>true</open-source>
    <compatibility>
        <game-type>OSRS</game-type>
    </compatibility>
    <categories>
        <category>MINING</category>
    </categories>
	<resources>
        <resource>com/runemate/geashawscripts/LazyAIOMiner/gui/LazyAIOMinerGUI.fxml</resource>
    </resources>
    <!--Required to publish on the bot store-->
    <internal-id>lazyaiominer</internal-id>
</manifest>